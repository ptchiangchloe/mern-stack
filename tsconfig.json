{
    "compilerOptions": {
        "outDir": "./static/",
        "noImplicitAny": true,
        "module": "es6",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "allowSyntheticDefaultImports": true,
        "sourceMap": true,
        "moduleResolution": "node",
    },
    "exclude": [
        "node_modules",
        "dist"
    ],
    "compileOnSave": false
}